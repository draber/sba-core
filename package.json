{
  "name": "spelling-bee-assistant",
  "version": "5.0.0",
  "description": "Spelling Bee Assistant is an add-on for Spelling Bee, the New York Timesâ€™ popular word puzzle",
  "homepage": "https://github.com/draber/sba-core.git#readme",
  "author": "Dieter Raber",
  "license": "GPL-3",
  "scripts": {
    "js:once": "rollup -c ./src/config/rollup.js",
    "js:watch": "rollup -c -w ./src/config/rollup.js",
    "js:build": "npm run css:build && npm run feConf:build && rollup -c ./src/config/rollup.js --config-env=prod",
    "feConf:once": "node ./src/modules/config-segments/fe-fonfig.js",
    "feConf:build": "node ./src/modules/config-segments/fe-fonfig.js --env=prod",
    "css:once": "postcss ./src/css/main.css --config=src/config/postcss --verbose -d ./src/css/build",
    "css:watch": "postcss ./src/css/main.css --config=src/config/postcss -d ./src/css/build -w",
    "css:build": "postcss ./src/css/main.css --config=src/config/postcss -d ./src/css/build --env=prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/draber/sba-core.git"
  },
  "bugs": {
    "url": "https://github.com/draber/sba-core.git/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "a-nicer-console": "^1.0.2",
    "acorn-import-assertions": "^1.8.0",
    "cssnano": "^5.1.7",
    "fs-extra": "^10.0.1",
    "minimist": "^1.2.6",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-discard-comments": "^5.1.1",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.4.3",
    "postcss-scss": "^4.0.3",
    "remove-accents-diacritics": "^1.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-import-assert": "^2.1.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.12.1",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "fancy-node": "^1.0.12"
  }
}
